    "src": "src"
  },
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "dragselect": "^2.7.4"
  },
  "devDependencies": {
    "@blockly/dev-scripts": "^4.0.5",
    "@blockly/dev-tools": "^8.0.7",
